    "@storybook/addon-links": "^6.0.16",
    "@storybook/vue": "^6.0.16",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "^4.5.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "react-is": "^16.13.1",
